{
  "name": "proof-of-concept-studio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev --filter web --filter api --filter runner",
    "lint": "pnpm -r lint",
    "build": "pnpm -r build",
    "briefs:apac": "pnpm --filter @proof/runner run run:apac",
    "briefs:international": "pnpm --filter @proof/runner run run:international",
    "briefs:au": "pnpm --filter @proof/runner run run:au",
    "briefs:us": "pnpm --filter @proof/runner run run:us",
    "agents:list": "pnpm --filter @proof/runner run agents:list"
  }
}
